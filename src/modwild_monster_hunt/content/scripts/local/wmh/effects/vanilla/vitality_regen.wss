

class WMH_EffectVitalityRegenBonus extends WMH_VanillaEffect {
  default showOnHUD = true;

  default isPositive = true;
  default isNeutral = false;
  default isNegative = false;

  public function initVitalityRegenBonusOnPlayer(
    owner: CGameplayEntity,
    optional duration: float
  ) {
    this.initOnPlayer(owner, 'WMHeffectVitalityRegenBonus', duration);
  }
}
