

class WMH_EffectCriticalBonus extends WMH_VanillaEffect {
  default showOnHUD = true;

  default isPositive = true;
  default isNeutral = false;
  default isNegative = false;

  public function initCriticalBonusOnPlayer(
    owner: CGameplayEntity,
    optional duration: float
  ) {
    this.initOnPlayer(owner, 'WMHeffectCriticalBonus', duration);
  }
}
