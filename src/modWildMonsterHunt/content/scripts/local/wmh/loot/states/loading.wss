state Loading in WMH_LootManager {
  // needed in some of the methods, to avoid calling GetWitcherPlayer non stop
  // it is cached.
  var witcher: W3PlayerWitcher;

  event OnEnterState(previous_state_name: name) {
    super.OnEnterState(previous_state_name);
    this.Loading_main();
  }

  entry function Loading_main() {
    // WMHINFO("WMH_LootManagerStateLoading:Loading_main()");

    var empty: array<name>;
    var i: int;

    parent.loot_by_category.Clear();
    parent.loot_by_category_rollers.Clear();
    for (i = 0; i < EnumGetMax('WMH_LootTag') + 1; i += 1) {
      parent.loot_by_category.PushBack(empty);
      parent.loot_by_category_rollers.PushBack(new WMH_RatiosRoller in parent);
    }

    parent.loot_category_roller = new WMH_RatiosRollerGen<WMH_LootTag> in parent;
    parent.loot_category_roller.addRatio(4, WMH_LootTag_Food);
    parent.loot_category_roller.addRatio(1, WMH_LootTag_Gear);
    parent.loot_category_roller.addRatio(4, WMH_LootTag_Herb);
    parent.loot_category_roller.addRatio(2.5, WMH_LootTag_Alchemy);
    parent.loot_category_roller.addRatio(2, WMH_LootTag_Material);
    parent.loot_category_roller.addRatio(1, WMH_LootTag_Ore);
    parent.loot_category_roller.addRatio(0.5, WMH_LootTag_Recipe);
    parent.loot_category_roller.addRatio(0.5, WMH_LootTag_Schematic);

    this.witcher = GetWitcherPlayer();

    this.registerLootFood();
    this.registerLootGear();
    this.registerLootHerb();
    this.registerLootAlchemy();
    this.registerLootMaterial();
    this.registerLootOre();

    this.registerWmhRecipes();
    this.registerRecipesMisc();
    this.registerRecipesTier1();
    this.registerRecipesTier2();
    this.registerRecipesTier3();
    this.registerSchematicsMisc();
    this.registerSchematicsTier0();
    this.registerSchematicsTier1();
    this.registerSchematicsTier2();
    this.registerSchematicsTier3();
    this.registerSchematicsTier4();
    this.registerSchematicsTier5();
    this.registerSchematicsTierNGP();

    this.registerRopPerks();

    parent.GotoState('Waiting');
  }

