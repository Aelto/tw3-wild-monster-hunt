  private function tryAddRecipe(recipe: name) {
    if (
      this.witcher.CanLearnAlchemyRecipe(recipe)
      && !this.witcher.WMH_isRecipeKnown(recipe)
    ) {
      // the loot level is not important, we short-circuit the functions if the
      // tier is too high instead so this method is never called.
      parent.registerLoot(WMH_LootTag_Recipe, 0, recipe);
    }
  }

  private function tryAddSchematic(schematic: name) {
    if (!this.witcher.WMH_isSchematicKnown(schematic)) {
      // the loot level is not important, we short-circuit the functions if the
      // tier is too high instead so this method is never called.
      parent.registerLoot(WMH_LootTag_Schematic, 0, schematic);
    }
  }

  private function addPerkRatio(perk: ESkill, ratio: float) {
    parent.runes_of_power_perks.PushBack(perk);
    parent.runes_of_power_perks_roller.addRatio(ratio);
  }
