state Running in WMH_Master {
	event OnEnterState(previous_state_name: name) {
		super.OnEnterState(previous_state_name);
		this.Running_main();
	}

	entry function Running_main() {
		WMHINFO("WMH_MasterStateRunning:Running_main()");
		parent.spawn_points.maybeRefresh();
		parent.bestiary.maybeRefresh(parent.biomes);
		parent.onSpawn();
		parent.GotoState('Waiting');
	}
}