class WMH_LifestealEffect extends WMH_Effect {
  public function initLifesteal(
    target: CGameplayEntity,
    duration: float,
  ) {
    this.owner = owner;
    this.target = target;
    this.duration = duration;
    this.sourceName = "WMH_LifestealEffect";
    this.targetEffectManager = effect_manager;

    this.EntityHandleSet(this.creatorHandle, this.owner);

    this.isOnPlayer = target as CPlayer;

    this.CalculateDuration(true);
  }
}