

class WMH_EffectExperienceBonus extends CBaseGameplayEffect {
  default showOnHUD = true;

  default isPositive = true;
  default isNeutral = false;
  default isNegative = false;

  public function initExperienceBonusOnPlayer(
    owner: CGameplayEntity,
    optional duration: float
  ) {
    this.initOnPlayer(owner, 'WMHeffectExperienceBonus', duration);
  }
}
